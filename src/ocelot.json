      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE", "OPTIONS" ]
    },
    {
      "DownstreamPathTemplate": "/api/questions/domain/",
      "DownstreamScheme": "http",
      "ServiceName": "QuestionBank",
      "UpstreamPathTemplate": "/questionbank/domain/",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE", "OPTIONS" ]
    },
    {
      "DownstreamPathTemplate": "/api/questions/domain/{url}",
      "DownstreamScheme": "http",
      "ServiceName": "QuestionBank",
      "UpstreamPathTemplate": "/questionbank/domain/{url}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE", "OPTIONS" ]
    },
    {
      "DownstreamPathTemplate": "/api/conceptmap/",
      "DownstreamScheme": "http",
